{
    "manifest_version": 3,
    "name": "Calculator",
    "version": "1.0",
    "description": "A useful tool for basic arithmetic operations",
    "action": {
        "default_icon": "tent.png",
        "default_popup": "popup/popup.html" 
      },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs"]
}